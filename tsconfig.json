{
    "compilerOptions": {
        "module": "ES2015",
        "target": "es6",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "outDir": "./dist",
        "rootDir": "./src/",
        "allowSyntheticDefaultImports": true
    },
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}
